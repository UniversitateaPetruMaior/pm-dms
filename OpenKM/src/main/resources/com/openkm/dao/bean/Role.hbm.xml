<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.Role" table="OKM_ROLE">
    <cache usage="nonstrict-read-write"/>
    <id name="id" column="ROL_ID" length="64"><generator class="assigned"/></id>
    <property name="active" column="ROL_ACTIVE" type="true_false" not-null="true"/>
  </class>
</hibernate-mapping>
